    "email": "mehmet@mehmeterim.com"
  },
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.0.3",
    "@angular-devkit/schematics": "^8.0.6",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.800.6"
